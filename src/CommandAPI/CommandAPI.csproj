    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>348863f1-dd6d-4f3f-bd60-bf8450fa1325</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="11.0.0" />
